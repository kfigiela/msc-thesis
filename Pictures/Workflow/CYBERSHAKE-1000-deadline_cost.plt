      set autoscale y
      set autoscale y2

      set yrange [0:]
      set xrange [0:15]
      unset grid
      unset key      

      set xlabel 'Time limit (hours)'
      set ylabel 'Cost ($  )'
      set term pdf monochrome font "Helvetica,12"
      set out "./CYBERSHAKE-1000-deadline_cost.pdf"

